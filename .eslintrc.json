{
    "extends": "eslint:recommended",
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
        }
    },
    "rules": {
        "semi": "error",
        "no-unused-vars": ["warn", { "args": "none" }]
    },
    "globals": {
        "PIXI": true
    }
}
